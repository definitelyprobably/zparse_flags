	# -ho -hhoA -o -mM -mN -oB --opt=C -h -oD --man=O --man P --help -m Q \
	# A -- -m Q -q --q -- P
	#~~~~ zparse_flags_is_IAU_S ~~~~#
	zparse_flags_is_IAU_S flag_aux_B1 2>/dev/null
	ret_val=$?
	((ret_val != 2)) && \
		failed_funcs+=(zparse_flags_is_IAU_S:B1)

	zparse_flags_is_IAU_S flag_aux_B1 1 2>/dev/null
	ret_val=$?
